<body>
    
<h1 align="center">Number Sets<br>
<font size=2><%= link_to "(Add More)", :action => "new" %></font>
</h1>


<table align="center">
  <tr>
    <% @sets.each do |set| %>
      <td align=center>
        <h3><%= set.set %>
          <font size=1>
            <%= link_to "Edit", :action => "edit", :id => set.id %>
            <%= link_to "Delete", :action => "delete", :id => set.id %>
          </font>
        </h3>
        Set owner: <%= set.user.name %><br><br>
        Associated Emails: 
        
        <% set.emails.each do |email| %>
          <br><%= email.email %>
        <% end %>
        
      </td>
    <% end %>
  </tr>
</table>


</body>