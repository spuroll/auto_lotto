<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <div class="logo"><%= link_to "Auto Lotto", :action => "index" %></div>
    <nav>
      <ul class="nav navbar-nav pull-right">
        <div class="navlinks"><%= link_to "Active Tickets", :action => "index"%> | |
        <%= link_to "Expired Tickets", :action => "expired"%> | | 
        <%= link_to "Logout", {:controller => 'access', :action => 'logout'} %></div>
      </ul>
    </nav>
  </div>
</header>
<br><br>
<%= form_for(:number_set, :url => {:action => "update", :id => @number_set.id}) do |f| %>
  
  <table align = center>
    <tr><th>Modify number set (## ## ## ## ## ##):</th>
    <td><%= f.text_field :set %></td></tr>
    <tr><th>Modify draw date (YYYY-MM-DD):</th>
    <td><%= f.text_field :check_date, :value => @number_set.check_date.to_date %></td></tr>
  </table>

  <div align=center><%= submit_tag("Edit Entry") %></div>
<% end %>